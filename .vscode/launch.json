    "version": "0.2.0",
    "configurations": [

        {
            "type": "node",
            "request": "launch",
            "name": "Production Run",
            "program": "${workspaceFolder}/src/main.js",
            "args": [
                "-sp=/dev/ttyACM1",
                "-prod",
                "--loggerTime=60000",
                "--iface=wlp4s0"
            ]
            
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Production Scan",
            "program": "${workspaceFolder}/src/main.js",
            "args": [
                "-sp=scan",
                "-prod",
                "--loggerTime=60000",
                "--iface=wlp4s0"
            ],
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Demo run",
            "program": "${workspaceFolder}/src/main.js",
            "args": [
                "--loggerTime=60000"
            ]
            
        }
    ]
}